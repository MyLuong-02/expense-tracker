# Required for AI expense parsing
GEMINI_API_KEY=your_gemini_api_key

# Required for authentication
JWT_SECRET=your_random_secret_key_min_32_chars

# OAuth - get from Google Cloud Console (APIs & Services > Credentials)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# OAuth - get from Facebook Developer (My Apps > Settings > Basic)
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# Base URL for OAuth redirect (use http://localhost:3000 for dev)
BASE_URL=http://localhost:3000
